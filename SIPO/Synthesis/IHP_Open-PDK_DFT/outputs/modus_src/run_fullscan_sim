#!/bin/ksh
 
# Template script for running Cadence Xcelium simulator (XMCSIM) on Cadence Modus DFT Software Solution generated test vectors.
 
#  Generated by Genus(TM) Synthesis Solution 23.11-s100_1
 
export WORKDIR=outputs/modus_src
 
xrun \
   +access+rwc \
   +xmstatus \
   +xm64bit \
   +TESTFILE1=$WORKDIR/testresults/verilog/VER.FULLSCAN.serial_to_parallel_atpg.data.scan.ex1.ts1.verilog \
   +TESTFILE2=$WORKDIR/testresults/verilog/VER.FULLSCAN.serial_to_parallel_atpg.data.logic.ex1.ts2.verilog \
   +HEARTBEAT \
   +FAILSET \
   +xmtimescale+1ns/1ps \
   +xmoverride_timescale \
   +xmseq_udp_delay+2ps \
   +libext+.v+.V+.z+.Z+.gz \
   +xmlibdirname+$WORKDIR/Inca_libs_19_44_48 \
   -l $WORKDIR/ncverilog_FULLSCAN.log \
  -v /home/bas33767/Desktop/IHP-Open-PDK/ihp-sg13g2/libs.ref/sg13g2_stdcell/verilog/sg13g2_stdcell.v \
$WORKDIR/serial_to_parallel.test_netlist.v \
$WORKDIR/testresults/verilog/VER.FULLSCAN.serial_to_parallel_atpg.mainsim.v

